    </reference>
    <output>
      <prop>/controls/flight/rudder</prop>
    </output>
    <config>
      <Kp>0.25</Kp>
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>10.0</Ti>
      <Td>0.000000001</Td>
      <u_min>-0.25</u_min>
      <u_max>0.25</u_max>
    </config>
  </pid-controller>


 <!-- =============================================================== -->
 <!-- Pitch Axis Modes                                                -->
 <!-- =============================================================== -->

 <pi-simple-controller>
    <name>TO/GA</name>
    <debug>false</debug>
    <enable>
      <prop>instrumentation/afds/ap-modes/pitch-mode</prop>
      <value>TO/GA</value>
    </enable>
    <input>
