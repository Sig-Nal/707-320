    <reference>
      <prop>/autopilot/internal/target-roll-deg</prop>
    </reference>
    <output>
      <prop>/controls/flight/aileron</prop>
    </output>
    <config>
      <Kp>0.1</Kp><!-- original 0.025 22 bien-->
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
