				</flaps>


<!--Systems-->

        <systems>
        <path>Aircraft/707-320/Systems/systems.xml</path>
	            <autopilot>
                <path>Aircraft/707-320/Systems/707-autopilot.xml</path>
            </autopilot>
        </systems>
	
        <instrumentation>
            <path>Aircraft/707-320/Systems/instrumentation.xml</path>
