<PropertyList>

    <sim>
        <description>Boeing 707-3J9C</description>
        <author>Innis Cunningham, Prestes, Andino Gonth√§ler</author>
        <status>alpha</status>
        <aircraft-version>1.2</aircraft-version>
        <flight-model>yasim</flight-model>
        <aero>707-320</aero>
        <fuel-fraction>0.75</fuel-fraction>
        <startup>
            <splash-texture>Aircraft/707-320/splash2.png</splash-texture>
        </startup>

        <model>
            <path>Aircraft/707-320/Models/707-3J9C.xml</path>
            <livery>
                <file type="string">SAHA</file>
            </livery>
            <lights>
                <file type="string">SAHA</file>
            </lights>
            <armrest type="double">0</armrest>
        </model>


        <hud>
            <path n="1">Huds/default.xml</path>
            <visibility n="1">false</visibility>
            <enable3d n="1">false</enable3d>
        </hud>


<!--Views-->     

	<view n="0">
            <name>Pilot View</name>
            <type>lookfrom</type>
            <internal type="bool">true</internal>
            <config>
                <x-offset-m type="double">-0.49</x-offset-m> <!-- original -0.36 L/R-->
                <y-offset-m type="double">-0.70</y-offset-m>   <!-- original -0.67 U/D-->
                <z-offset-m type="double">-18.35</z-offset-m>  <!-- original -18.36 F/B-->
                <pitch-offset-deg>0</pitch-offset-deg> <!-- original -8.6 -->
                <default-field-of-view-deg type="double">84.7</default-field-of-view-deg> <!-- original 84.7 -->
                <limits>
                    <enabled type="bool">true</enabled>
                    <left>
                        <heading-max-deg type="double">140</heading-max-deg>
                        <x-offset-max-m type="double">0.35</x-offset-max-m>
                        <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </left>
                    <right>
                        <heading-max-deg type="double">160</heading-max-deg>
                        <x-offset-max-m type="double">0.25</x-offset-max-m>
                        <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </right>
                </limits>
            </config>
        </view>


 <view n="700">
            <name>Radio/CDU Panel View</name>
            <enabled type="bool">true</enabled>
            <type>lookfrom</type>
            <internal type="bool">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
                <default-field-of-view-deg type="double">40.0</default-field-of-view-deg>
                <default-pitch-deg type="double">-90</default-pitch-deg>
                <default-heading-deg type="double">0</default-heading-deg>
                <front-direction-deg type="double">0</front-direction-deg>
                <front-left-direction-deg type="double">45</front-left-direction-deg>
                <left-direction-deg type="double">90</left-direction-deg>
                <back-left-direction-deg type="double">135</back-left-direction-deg>
                <back-direction-deg type="double">180</back-direction-deg>
                <back-right-direction-deg type="double">225</back-right-direction-deg>
                <right-direction-deg type="double">270</right-direction-deg>
                <front-right-direction-deg type="double">315</front-right-direction-deg>
                <dynamic-view type="bool">true</dynamic-view>
               <x-offset-m type="double">0.00</x-offset-m> <!-- original -0.36 L/R-->
                <y-offset-m type="double">-0.70</y-offset-m>   <!-- original -0.67 U/D-->
                <z-offset-m type="double">-18.35</z-offset-m>  <!-- original -18.36 F/B-->
                <pitch-offset-deg>-90</pitch-offset-deg>
                <limits>
                    <enabled type="bool">true</enabled>
                    <left>
                      <heading-max-deg type="double">140</heading-max-deg>
                      <x-offset-max-m type="double">0.25</x-offset-max-m>
                      <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </left>
                    <right>
                        <heading-max-deg type="double">160</heading-max-deg>
                        <x-offset-max-m type="double">0.35</x-offset-max-m>
                        <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </right>
                </limits>
            </config>
        </view>

    <view n="600">
        <name>CoPilot View</name>
        <enabled type="bool">true</enabled>
        <type>lookfrom</type>
        <internal type="bool">true</internal>
        <config>
            <from-model type="bool">true</from-model>
            <from-model-idx type="int">0</from-model-idx>
            <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
            <default-field-of-view-deg type="double">77.7</default-field-of-view-deg>
            <default-pitch-deg type="double">0</default-pitch-deg>
            <default-heading-deg type="double">0</default-heading-deg>
            <front-direction-deg type="double">0</front-direction-deg>
            <front-left-direction-deg type="double">45</front-left-direction-deg>
            <left-direction-deg type="double">90</left-direction-deg>
            <back-left-direction-deg type="double">135</back-left-direction-deg>
            <back-direction-deg type="double">180</back-direction-deg>
            <back-right-direction-deg type="double">225</back-right-direction-deg>
            <right-direction-deg type="double">270</right-direction-deg>
            <front-right-direction-deg type="double">315</front-right-direction-deg>
            <dynamic-view type="bool">true</dynamic-view>
            <x-offset-m type="double">0.49</x-offset-m>
            <y-offset-m type="double">-0.70</y-offset-m>
            <z-offset-m type="double">-18.35</z-offset-m>
            <pitch-offset-deg>-8.6</pitch-offset-deg>
            <limits>
                <enabled type="bool">true</enabled>
                <left>
                  <heading-max-deg type="double">140</heading-max-deg>
                  <x-offset-max-m type="double">0.25</x-offset-max-m>
                  <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                </left>
                <right>
                    <heading-max-deg type="double">160</heading-max-deg>
                    <x-offset-max-m type="double">0.35</x-offset-max-m>
                    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                </right>
            </limits>
        </config>
    </view>


		<view n="200">
			<config>
				
				<z-offset-m type="double" archive="y">-50.0</z-offset-m>
			</config>
		</view>

		<chase-distance-m>-45</chase-distance-m>

		<view n="300">
		    <name>Left Wing</name>
		    <type>lookfrom</type>
		    <internal archive="y">true</internal>
		    <config>
		      <from-model type="bool">true</from-model>
		      <from-model-idx type="int">0</from-model-idx>
		      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
		      <default-field-of-view-deg type="double">55</default-field-of-view-deg>
		      <pitch-offset-deg>-8.0</pitch-offset-deg>
		      <heading-offset-deg type="double">90</heading-offset-deg> <!-- parallel to the panel -->
		      <x-offset-m archive="y"> -2.0 </x-offset-m> <!--Left/Right -->
		      <y-offset-m archive="y"> 0.30 </y-offset-m> <!--Up/Down -->
		      <z-offset-m archive="y"> 4.00 </z-offset-m> <!--Fdw/Back -->
		    </config>
	    	</view>
		<view n="400">
		    <name>Right Engine</name>
		    <type>lookfrom</type>
		    <internal archive="y">true</internal>
		    <config>
			      <from-model type="bool">true</from-model>
			      <from-model-idx type="int">0</from-model-idx>
			      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
			      <default-field-of-view-deg type="double">45</default-field-of-view-deg>
			      <pitch-offset-deg>-8.0</pitch-offset-deg>
			      <heading-offset-deg type="double">225</heading-offset-deg> <!-- parallel to the panel -->
			      <x-offset-m archive="y"> 2.0 </x-offset-m> <!--Left/Right -->
			      <y-offset-m archive="y"> 0.30 </y-offset-m> <!--Up/Down -->
			      <z-offset-m archive="y"> -10.00 </z-offset-m> <!--Fdw/Back -->
		    </config>
	    	</view>


<!--Start Offset-->

    <airport>
        <runways>
            <start-offset-m>50</start-offset-m>
        </runways>
    </airport>

        <chase-distance-m>-100</chase-distance-m>

<!--Flaps-->


				<flaps>
					<setting>0.000</setting>
					<setting>0.125</setting>
					<setting>0.250</setting>
