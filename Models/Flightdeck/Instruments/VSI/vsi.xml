<PropertyList>

 <path>vsi.ac</path>
 

<animation>
		<type>material</type>
		<!--object-name>vsibezel</object-name-->
		<object-name>vsiface</object-name>
  		<object-name>light</object-name>
  		<object-name>needle</object-name>
		<emission>
			<red>0.7</red>
			<green>0.7</green>
			<blue>0.7</blue>
			<!--factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop--> <!--para cuando haya luz -->
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
		</emission>
	</animation>


